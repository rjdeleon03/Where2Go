    "debug": "~2.6.9",
    "express": "^4.16.3",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "mongoose": "^5.0.12",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3"
  }
}
